{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ES2023"
    ],
    "module": "ES2022",
    "moduleResolution": "Node",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "noEmit": true
  },
  "exclude": [
    "**.test.ts"
  ]
}